.animate-repeat,
.animate-repeat.ng-move,
.animate-repeat.ng-enter,
.animate-repeat.ng-leave {
    -webkit-transition:all linear 0.3s;
    transition:all linear 0.3s;
}

.body-wrapper.no-animations .animate-repeat,
.body-wrapper.no-animations .animate-repeat.ng-move,
.body-wrapper.no-animations .animate-repeat.ng-enter,
.body-wrapper.no-animations .animate-repeat.ng-leave {
    -webkit-transition: none;
    transition: none;
}
.animate-repeat.ng-leave.ng-leave-active,
.animate-repeat.ng-move,
.animate-repeat.ng-enter {
    opacity:0;
    transform: rotateX(90deg);
    -webkit-transform: rotateX(90deg);
    transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
}
.animate-repeat.ng-leave,
.animate-repeat.ng-move.ng-move-active,
.animate-repeat.ng-enter.ng-enter-active {
    opacity:1;
    transform: rotateX(0deg);
    -webkit-transform: rotateX(0deg);
    transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
}


.animate-repeat.ng-leave,
.animate-repeat.ng-leave-stagger {
    -webkit-transition-duration: 0s;
    transition-duration: 0s;
}
.animate-repeat.ng-enter-stagger {
    -webkit-transition-duration: 0s;
    transition-duration: 0s;
}
.body-wrapper.no-animations .animate-repeat.ng-leave,
.body-wrapper.no-animations .animate-repeat.ng-leave-stagger,
.body-wrapper.no-animations .animate-repeat.ng-enter-stagger {
    -webkit-transition-duration: inherit;
    transition-duration: inherit;
}
