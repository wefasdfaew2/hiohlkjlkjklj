.body-wrapper.modern.sport .right-options-j .factor-change-wrapper > p {
    color: #4C4D56;
}
.body-wrapper.freebet .games-center .md-progress-circular {
    margin-top: 250px;
}
.betslip-a-rules-j {
    font: 13px/24px "NotoSans", "NotoSans2";
    color: #737888;
    margin-left: 8px;
    display: inline-block;
}
.dndDraggingSource {
    display: none;
}
.game-info .sport-title .curr-minute-info:first-letter {
    text-transform: uppercase
}
.popup-bg-contain .live-casino .fedback-info h3 span {
    display: inline-block;
    width: auto;
}

.fedback-wrapper.live-casino .fedback-info ul li.jackpot-current-user, .fedback-wrapper.live-casino .fedback-info ul li.jackpot-current-user p {color: #A61F67;font-weight: bold;}
.sport-news-link-t {
    position: relative;
    padding-right: 7px;
}
.sport-news-link-t:after {
    content: '1';
    font-size: 8px;
    line-height: 10px;
    text-align: center;
    display: block;
    background: #A61F67;
    width: 10px;
    height: 10px;
    border-radius: 10px;
    position: absolute;
    top: -3px;
    right: -6px;
    text-indent: -1px;
}
.sport-news-link-t:hover:after, .new-homepage .nav-news-sport ul li a:hover .sport-news-link-t:after {
    color: white;
}
.check.terms label a.info-tooltip {
    cursor: default;
}
.hp-bottom-buner-j {
    float: left;
    width: 540px;
    margin-left: 10px;
    overflow: hidden;
    position: relative;
    height: 367px;
    background: #111522;
    border-bottom: 2px solid #111522;
}
.hp-bottom-buner-j .md-progress-circular {
    margin: -50px;
    position: absolute;
    top: 50%;
    left: 50%;
}
@media screen and (max-width: 1260px) {
    .new-homepage .hp-bottom-buner-j {
        display: none;
    }
}
