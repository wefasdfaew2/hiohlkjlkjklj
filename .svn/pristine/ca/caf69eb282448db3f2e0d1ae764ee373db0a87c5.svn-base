.tl-container .timeline-control{
    top: 12px;
    height:32px;
}
.timeline-container{
    margin:12px 0;
    height:40px;
    position: relative;
}
.timeline-control{
    position: absolute;
    /*top: 12px;*/
    width: 97%;
    height:100%;
    /*height: 32px;*/
    color: #fff;
    text-align: left;
    font: 12px/12px arial;
    box-shadow: 1px 0 0 #fff inset, -1px 0 0 #fff inset;
}
.timeline-control:before{
    content: "\00a0 0";
    position: absolute;
    left: 0;
    width: 103%;
    top: 4px;
    top: calc(50% - 12px);

    border-bottom: 1px solid #fff;
    box-shadow: 0 -6px 6px rgba(0, 0, 0, 0.3) inset, 0 6px 6px rgba(0, 0, 0, 0.3);
}
.timeline-control:after {
    content: "9 0";
    position: absolute;
    left: 97.5%;
    left: calc(100% - 9px);
    top: 4px;
    top: calc(50% - 12px);
    white-space: nowrap;
}
.timeline-control span.tl-20,
.timeline-control span.tl-40,
.timeline-control span.tl-6-0,
.timeline-control span.tl-80{
    display: none;
}
.timeline-control span{
    position: absolute;
    top: 0;
    width: 11.11111%;
    width: calc(100% / 9);
    height:100%;
    box-shadow: -1px 0 0 #fff inset;
    text-shadow: 1px 1px 0 #000;
}
.timeline-control span:before{
    position:absolute;
    left:75%;
    left: calc(100% - 9px);
    top: 4px;
    top: calc(50% - 12px);
    white-space: nowrap;
}
.timeline-control span:after{
    content: "";
    position: absolute;
    top: 25%;
    width: 50%;
    height: 50%;
    box-shadow: 1px 0 0 #fff inset, -1px 0 0 #fff inset;
}
.timeline-control span.tl-15{left:5.55555%; left: calc(100% / 18);}
.timeline-control span.tl-30{left:22.22222%;left: calc((100% / 18) * 4);}
.timeline-control span.tl-45{left:38.88888%;left: calc((100% / 18) * 7); box-shadow: -2px 0 0 #fff inset;}
.timeline-control span.tl-60{left:55.55555%;left: calc((100% / 18) * 10);}
.timeline-control span.tl-75{left:72.22222%;left: calc((100% / 18) * 13);}
.timeline-control span.tl-90{left:88.88888%;left: calc((100% / 18) * 16); box-shadow: -2px 0 0 #fff inset;}
.timeline-control span.ht-ft, .timeline-control.tl-80 span.ht-ft{
    width:50%;
    left: 50%;
    font: 10px/10px arial;
    font-weight: bold;
}
.timeline-control span.tl-15:before{content: "1 5";}
.timeline-control span.tl-30:before{content: "3 0";}
.timeline-control span.tl-45:before{content: "4 5";}
.timeline-control span.tl-60:before{content: "6 0";}
.timeline-control span.tl-75:before{content: "7 5";}
.timeline-control span.ht-ft:before{
    content: "H T";
    left: -9px;
    top: -10px;
    box-shadow: none;
}
.timeline-control span.ht-ft:after{
    content: "F T";
    left: 96.5%;
    left: calc(100% - 9px);
    top: -10px;
    box-shadow:none;
}
/* 80 minutes timeline */
.timeline-control.tl-80 span.tl-15,
.timeline-control.tl-80 span.tl-30,
.timeline-control.tl-80 span.tl-45,
.timeline-control.tl-80 span.tl-60,
.timeline-control.tl-80 span.tl-75,
.timeline-control.tl-80 span.tl-90{
    display: none;
}
.timeline-control.tl-80 span.tl-20,
.timeline-control.tl-80 span.tl-40,
.timeline-control.tl-80 span.tl-6-0,
.timeline-control.tl-80 span.tl-80{
    display: block;
}
.timeline-control.tl-80:after {content: "8 0";}
.timeline-control.tl-80 span{width:16.66666%; width: calc(100% / 6);}
.timeline-control.tl-80 span.tl-20{left:8.33333%; left: calc(100% / 12);}
.timeline-control.tl-80 span.tl-40{left:33.33333%; left: calc((100% / 12) * 4); box-shadow: -2px 0 0 #fff inset;}
.timeline-control.tl-80 span.tl-6-0{left:58.33333%; left: calc((100% / 12) * 7);}
.timeline-control.tl-80 span.tl-80{left:83.33333%; left: calc((100% / 12) * 10); box-shadow: -2px 0 0 #fff inset;}
.timeline-control.tl-80 span.tl-20:before{content: "2 0";}
.timeline-control.tl-80 span.tl-40:before{content: "4 0";}
.timeline-control.tl-80 span.tl-6-0:before{content: "6 0";}

/* extra time */
.timeline-control.extra span.tl-15:before{content: " 5";}
.timeline-control.extra span.tl-30:before{content: "1 0";}
.timeline-control.extra span.tl-45:before{content: "1 5";}
.timeline-control.extra span.tl-60:before{content: "2 0";}
.timeline-control.extra span.tl-75:before{content: "2 5";}
.timeline-control.extra:after {content: "3 0";}
.timeline-control.tl-80.extra span.tl-20:before{content: "5";}
.timeline-control.tl-80.extra span.tl-40:before{content: "1 0";}
.timeline-control.tl-80.extra span.tl-6-0:before{content: "1 5";}
.timeline-control.tl-80.extra:after {content: "2 0";}
/* timeline control END */
