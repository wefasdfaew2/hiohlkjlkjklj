@import 'functions/colors';
@import '../../../css/classic';
@font-face {
  font-family: 'sirucaregular';
  src: url('../fonts/fsd_-_siruca.eot');
  src: url('../fonts/fsd_-_siruca.eot?#iefix') format('embedded-opentype'),
  url('../fonts/fsd_-_siruca.woff') format('woff'),
  url('../fonts/fsd_-_siruca.ttf') format('truetype'),
  url('../fonts/fsd_-_siruca.svg#sirucaregular') format('svg');
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: Helvetica;
  src: url('../fonts/arnamu.eot');
  src: local('Arian AMU'),
  url('../fonts/arnamu.eot?#iefix') format('embedded-opentype'),
  url('../fonts/arnamu.woff') format('woff'),
  url('../fonts/arnamu.ttf') format('truetype'),
  url('../fonts/arnamu.svg') format('svg');
  font-weight: normal;
  font-style: normal;
  unicode-range: U+530-58F, U+FB13-FB17;
}
body{font: 12px $font-f; background: $white-bg url(../images/tournament-bg.png) no-repeat 50% 360px fixed;}
/*header*/ 
.conteiner{
	box-shadow: 0 0 3px #333333;
	-moz-box-shadow: 0 0 3px #333333;
	-ms-box-shadow: 0 0 3px #333333;
	-o-box-shadow: 0 0 3px #333333;
	-webkit-box-shadow: 0 0 3px #333333;
}
.header-bg{
	background: #e63a35;
	background: -moz-linear-gradient(#e13a34, #181818);
    background: -webkit-linear-gradient(#e13a34, #181818);
    background: -o-linear-gradient(#e13a34, #181818);
    background: linear-gradient(#e13a34, #181818);
	box-shadow: 0 1px 2px #aaa inset;
	-moz-box-shadow: 0 1px 2px #aaa inset;
	-ms-box-shadow: 0 1px 2px #aaa inset;
	-o-box-shadow: 0 1px 2px #aaa inset;
	-webkit-box-shadow: 0 1px 2px #aaa inset;
}
@media screen and (max-width: 1100px){
.category-nav ul li:nth-child(n+6) a{
	background: #e63a35;
	background: -moz-linear-gradient(#e13a34, #181818);
    background: -webkit-linear-gradient(#e13a34, #181818);
    background: -o-linear-gradient(#e13a34, #181818);
    background: linear-gradient(#e13a34, #181818);
}
}
.conteiner.show-fix .logo, .conteiner.show-game .logo{
  background: url(../images/home.png) no-repeat 51px 8px;
  width: 88px;
  height: 41px;
}
.conteiner.show-fix .logo a, .conteiner.show-game .logo a{
  background: url(../images/logo2.png) no-repeat 0 2px;
  width: 68px;
  height: 41px;
  padding: 0 20px 0 0;
}
.top-black{background: $black-bg;}
.conteiner.show-fix .block-search, .conteiner.show-game .block-search{background: rgba(24, 24, 24, 0.5);} 
/*header end*/
.clock{background: url(../images/clock-icon.png) no-repeat 20px 50%;}
.clock span{
  font: 12px/14px $font-f;
  color: $header-text;
  text-transform: uppercase;
  white-space: nowrap;
}
.topmenu-sport-and-region {
  left: 230px;
  color: $header-text;
  font: 28px/25px 'sirucaregular', $font-f;
}
.language-change p{
  color: $header-text;
  text-transform: uppercase;
  font: 13px/34px $font-f;
}
.arrow-lang{background: url(../images/flag-arrow.png) no-repeat 0 0;}
.open-lang{
  background: $white-bg;
  box-shadow: 0 0 15px $shadow-color;
  -moz-box-shadow: 0 0 15px $shadow-color;
  -ms-box-shadow: 0 0 15px $shadow-color;
  -o-box-shadow: 0 0 15px $shadow-color;
  -webkit-box-shadow: 0 0 15px $shadow-color;
}
.set-icon{background: url(../images/settings.png) no-repeat 50% 50%;}
.open-lang .arrow-sport{background-size: contain;}
.settings-button .open-lang p{
  font: 13px/15px $font-f;
  font-weight: $bold-text;
  background: #dddddd;
}
.open-lang ul li{font: 13px/15px $font-f;}
.open-lang ul li:hover{color: $red-text;}
.open-lang ul li.active-format{
  color: $red-text;
  background: #eeeeee url(../images/active-format.png) no-repeat 6px 50%;
}
.help-b a{background: url(../images/help-bg.png) no-repeat 50% 50%;}
.prof-name a{
  color: #fffefe;
  font: 13px/16px $font-f;
  background: url(../images/arrow-name-bg.png) no-repeat 100% 50%;
}
.prof-name ul {
	background: $white-bg;
	box-shadow: 0 0 15px $shadow-color;
	-moz-box-shadow: 0 0 15px $shadow-color;
	-ms-box-shadow: 0 0 15px $shadow-color;
	-o-box-shadow: 0 0 15px $shadow-color;
	-webkit-box-shadow: 0 0 15px $shadow-color;
	&:before{			
			border-color: transparent transparent $white-bg transparent;
		}
	}
.prof-name ul li {
    border-top: 1px solid #eee;
    color: $lite-black-text;
    font: 13px/16px $font-f;
	background: none; 
	&:first-child{
		background: #eee;
		font-weight: bold;
		cursor: default;
		&:hover{
			color: $lite-black-text;
		}
	}
	&.message-icon{background: url("../images/user-icons.png") no-repeat  8px 11px;}   
	&.settings-icon{background: url("../images/user-icons.png") no-repeat 8px -26px;}
	&.logoff-icon{background: url("../images/user-icons.png") no-repeat 8px -62px;}
}
.prof-name ul li:hover{color:$red-text;}
/*logo*/
.logo{
  width: 152px;
  height: 58px;
  margin: 0 0 0 132px;
}
.logo a{
  background: url(../images/logo.png) no-repeat 50% 2px;
  width: 152px;
  height: 58px;
  padding: 0 5px;
}
/*menu*/
.main-nav ul li{background: $white-bg;}
.main-nav ul li.my-games{background: $white-bg url(../images/nav-icons.png) no-repeat 6px 8px;}
.animate-star-odd .star-anim, .animate-star-even .star-anim{
	background: url(../images/nav-icons.png) no-repeat 6px 8px;
}
.main-nav ul li .active-s{background: url(../images/arrow-nav.png) no-repeat 50% 100% !important;}
.main-nav ul li span{
  color: $black-text;
  text-transform: uppercase;
  font: 12px/14px $font-f;
  background: url(../images/nav-icons.png) no-repeat 10px -42px;
  font-weight: $bold-text;
}
.main-nav ul li a{
  color: $black-text;
  text-transform: uppercase;
  font: 12px/14px $font-f;
  font-weight: $bold-text;
}
.main-nav ul li span.my-games-count {
  background: none;
  font: 9px/10px $font-f;
  font-weight: normal;
  color: $white-text;
}
.main-nav ul li.my-bets{background: $white-bg url(../images/nav-icons.png) no-repeat 1px -164px;}
.main-nav ul li.balance{background: $white-bg url(../images/nav-icons.png) no-repeat 7px -42px;}
.not-sign li span{background: none !important;}
.main-nav ul li.fav-games{background: $white-bg url(../images/nav-icons.png) no-repeat 6px 8px;}
.conteiner.show-fix .main-nav ul.not-sign li:first-child, .conteiner.show-game .main-nav ul.not-sign li:first-child{background: $white-bg url(../images/nav-icons.png) no-repeat 6px 8px;}
.main-nav ul .last-child a{background: url(../images/nav-icons.png) no-repeat 10px -99px;}
.bottom-arrow{background: url(../images/arrow-w-block.png) no-repeat 0 0;}
.left-b{background: url(../images/bg-left-righ-win.png) no-repeat 0 0;}
.right-b{background: url(../images/bg-left-righ-win.png) no-repeat 0 -72px;}
.centr-text-b{
  background: url(../images/separator-bg-block-win.png) repeat-x 0 0;
  text-transform: uppercase;
  color: $white-text;
  font: 11px/18px $font-f;
  font-weight: $bold-text;
}
.conteiner.show-fix .category-nav, .conteiner.show-game .category-nav{background: $black-bg;}
.category-nav ul li{text-transform: uppercase;}
.category-nav ul li a{
  color: $white-text;
  font: 12px/14px $font-f;
}
.category-nav ul li a:hover, .category-nav ul .active a{background: $header-nav-bg;}
@media screen and (max-width: 1100px){
.conteiner.show-fix .category-nav ul li:nth-child(n+6) a, .conteiner.show-game .category-nav ul li:nth-child(n+6) a{background: $black-bg;box-shadow: 0 -1px 0 $shadow-color inset;}
.conteiner.show-fix .category-nav ul li:nth-child(n+6) a:hover, .conteiner.show-game .category-nav ul li:nth-child(n+6) a:hover, .category-nav ul:hover li:nth-child(6):before
 {background: $header-nav-bg;}
}
.conteiner.show-fix .help-b a, .conteiner.show-game .help-b a{background: url(../images/help-bg.png) no-repeat 50% 50%;}
/*slider*/
.block-slider{
  background: $bg-footer-slider;
  box-shadow: 0 5px 40px $shadow-color;
  -moz-box-shadow: 0 5px 40px $shadow-color;
  -ms-box-shadow: 0 5px 40px $shadow-color;
  -o-box-shadow: 0 5px 40px $shadow-color;
  -webkit-box-shadow: 0 5px 40px $shadow-color;
}
.closed-arrow-b{
  background: $shadow-color;
  background: rgba(11, 13, 12, 0.7);
}
.closed-arrow-b a{
  background: url(../images/closed-arrow.png) no-repeat 50% 0;
  color: $white-text;
}
/*slider end*/

/*menu-games*/
.menu-sport{background: url(../images/bg-menu-sport.png) repeat-x 0 0;}
.menu-sport .menu-sport-contain ul .active{background: url(../images/bg-menu-sport.png) repeat-x 0 -41px;}
.menu-sport .menu-sport-contain ul li a{
  color: #8b8a8a;
  font: 11px/14px $font-f;
  text-transform: uppercase;
  font-weight: $bold-text;
}
.menu-sport .menu-sport-contain ul .active a{color: $white-text;background: url(../images/sport-nav-hover.png) no-repeat 50% 0;}
.menu-sport .menu-sport-contain ul li a:hover{color: $white-text;}
/*menu-games*/
.menu-games{background: url(../images/bg-menu-games.png) repeat-x 0 -6px;}
.menu-games .contain-menu-games ul li{background: url(../images/bg-sport-nav-a.png) no-repeat 100% 0;}
.menu-games .contain-menu-games ul li.active{background: url(../images/bg-menu-games.png) repeat-x 0 -111px !important;}
.menu-games .contain-menu-games ul li a{
  color: $black-text;
  text-transform: uppercase;
  font: 11px/14px $font-f;
  font-weight: $bold-text;
  text-shadow: 1px 1px 1px $white-text;
}
.more-count.ng-binding:before{content: "+";}
.menu-games .contain-menu-games ul li.active a{
  color: $white-text;
  background: url(../images/arrow-sport-m.png) no-repeat 50% 100%;
  text-shadow: 1px 1px 1px $shadow-color;
}
@media screen and (max-width: 1200px){.conteiner.show-fix .topmenu-sport-and-region {left: 48px !important;}}
.menu-games .contain-menu-games ul li:first-child:hover{/*background: url(../images/bg-sport-nav-a.png) no-repeat 0 0;*/}
.menu-games .contain-menu-games ul li:first-child a{background: none;}
.menu-games .contain-menu-games ul li.active:first-child {background: url(../images/bg-menu-games.png) repeat-x 0 -111px;}
.menu-games .contain-menu-games ul li.active:first-child a{background: url(../images/arrow-sport-m.png) no-repeat 50% 100%;}

