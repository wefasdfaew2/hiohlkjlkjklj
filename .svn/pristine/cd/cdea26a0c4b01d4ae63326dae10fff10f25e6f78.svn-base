/*meny games*/
.menu-games .contain-menu-games ul .more-games{background: url(../images/more-icon.png) no-repeat 50% 10px;}
.menu-casino .menu-games .contain-menu-games ul .more-games{background: none;}
.more-games a, .menu-games .contain-menu-games ul .more-games a:hover{
  background: url(../images/more-arrow.png) no-repeat 99% 51px;
  color: $red-text;
  text-shadow: 1px 1px 1px $white-text;
}
.menu-games .contain-menu-games ul .more-games .contein-games, .country-b .country-block-contain ul .flag-more .contein-games{
  background: $white-bg;
  box-shadow: 0 0 60px $dark-shadow-color;
  -moz-box-shadow: 0 0 60px $dark-shadow-color;
  -ms-box-shadow: 0 0 60px $dark-shadow-color;
  -o-box-shadow: 0 0 60px $dark-shadow-color;
  -webkit-box-shadow: 0 0 60px $dark-shadow-color;
}
.arrow-sport{background: url(../images/dop-nav-arrow.png) no-repeat 0 0;}
.contein-games h3{
  font: 40px/44px 'sirucaregular', $font-f;
  color: $black-text;
}
.meny-games ul li{background: none !important;}
.menu-games .meny-games ul li a, .country-b .country-block-contain ul .flag-more .contein-games .meny-games ul li a{
  color: $black-text;
  font: 14px/16px $font-f;
  background: none;
  text-transform: none;
  font-weight: normal;
}
/*live button*/
.live-b{background: url(../images/bg-sport-nav-a.png) no-repeat 0 0;}
.menu-games .contain-menu-games ul li:first-child .live-b a{
  color: #244c00;
  font: 14px/16px $font-f;
  background: url(../images/live-b2-bg.png) no-repeat 11px 24px;
}
.menu-games .contain-menu-games ul li:first-child .live-b a:hover{
  color: #858585;
  text-shadow: 1px 1px 1px $white-text;
}
.live-b2{background: url(../images/bg-sport-nav-a.png) no-repeat 0 0;}
.menu-games .contain-menu-games ul li:first-child .live-b2 a{
  color: $red-text;
  font: 14px/16px $font-f;
  background: url(../images/tab-live.png) no-repeat 10px 24px;
}
.menu-games .contain-menu-games ul li:first-child .live-b2 a:hover{
  color: $red-text;
  text-shadow: 1px 1px 1px $white-text;
}
.menu-games .contain-menu-games ul li .pl-toggle{
	background: url(../images/bg-sport-nav-a.png) no-repeat 0 0;
	p {
    background: #222;
	background: rgba(0, 0, 0, 0.7);
    color: $white-text;
    font: 11px/11px $font-f;
    box-shadow: 0 0 15px $shadow-color, inset 0 1px 1px #555;
    -moz-box-shadow: 0 0 15px $shadow-color, inset 0 1px 1px #555;
    -ms-box-shadow: 0 0 15px $shadow-color, inset 0 1px 1px #555;
    -o-box-shadow: 0 0 15px $shadow-color, inset 0 1px 1px #555;
    -webkit-box-shadow: 0 0 15px $shadow-color, inset 0 1px 1px #555;
	border:1px solid $shadow-color;
    &:before {border-color: $shadow-color transparent transparent transparent;}
	}
	}
.menu-games .contain-menu-games ul li .pl-toggle .container{
  background: #ECECEC;
  box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.67), 0 1px 0 $white-bg;
  -moz-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.67), 0 1px 0 $white-bg;
  -ms-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.67), 0 1px 0 $white-bg;
  -o-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.67), 0 1px 0 $white-bg;
  -webkit-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.67), 0 1px 0 $white-bg;
}
.menu-games .contain-menu-games ul li .pl-toggle .container a{
  font: 12px/32px $font-f;
  font-weight: normal;
}
.menu-games .contain-menu-games ul li .pl-toggle .container a:nth-child(2) {
    color: #2F3330;
	background: rgba(148, 244, 98, 0.63);
	text-shadow: -1px -1px 0px #FFFFFF;
	font: 23px/36px "sirucaregular", $font-f; 
}
.menu-games .contain-menu-games ul li .pl-toggle .container a:first-child{
  color: $red-text;
  background: rgba(255, 255, 255, 0.4);
  font: 14px/29px "sirucaregular", $font-f;  
}
.menu-games .contain-menu-games ul li .pl-toggle .switch{
  box-shadow: inset 0px -3px 6px 0px rgba(0, 0, 0, 0.31), inset 0 -1px 0 rgba(0, 0, 0, 0.3);
  -moz-box-shadow: inset 0px -3px 6px 0px rgba(0, 0, 0, 0.31), inset 0 -1px 0 rgba(0, 0, 0, 0.3);
  -ms-box-shadow: inset 0px -3px 6px 0px rgba(0, 0, 0, 0.31), inset 0 -1px 0 rgba(0, 0, 0, 0.3);
  -o-box-shadow: inset 0px -3px 6px 0px rgba(0, 0, 0, 0.31), inset 0 -1px 0 rgba(0, 0, 0, 0.3);
  -webkit-box-shadow: inset 0px -3px 6px 0px rgba(0, 0, 0, 0.31), inset 0 -1px 0 rgba(0, 0, 0, 0.3);
  background: #edecf1;
}
.country-b  .country-block-contain ul .flag-more .contein-games .meny-games ul li a{
  font: 14px/16px $font-f;
  white-space: normal;
}
.country-b  .country-block-contain ul .flag-more .contein-games .meny-games ul li a:hover{color: $red-text !important;}
.meny-games ul li:first-child{
  color: $red-text;
  font: 16px/18px $font-f;
  font-weight: $bold-text;
  text-transform: uppercase;
}

/*search*/
.block-search input[type="text"]{
  color: #686767;
  font: 12px $font-f;
  background: $white-bg url(../images/bg-search.png) no-repeat 8px 50%;
}
.block-search input[type="text"]:focus {
  box-shadow: 0 0 5px $shadow-color;
  -moz-box-shadow: 0 0 5px $shadow-color;
  -ms-box-shadow: 0 0 5px $shadow-color;
  -o-box-shadow: 0 0 5px $shadow-color;
  -webkit-box-shadow: 0 0 5px $shadow-color;
}
.search-results-inner {
  background: $white-bg url(../images/icon-V.png) no-repeat 98% 7px;
  box-shadow: 0 0 60px $dark-shadow-color;
  -moz-box-shadow: 0 0 60px $dark-shadow-color;
  -ms-box-shadow: 0 0 60px $dark-shadow-color;
  -o-box-shadow: 0 0 60px $dark-shadow-color;
  -webkit-box-shadow: 0 0 60px $dark-shadow-color;
}
.search-results-inner h3{
  font: 20px/22px 'sirucaregular', $font-f;
  color: $black-text;
  text-transform: uppercase;
}
.search-no-results{
  font: 17px/20px $font-f;
  color: #C9B3B3;
}
#result-search{background: $white-bg;}
#result-search ul{margin: 0px;}
#result-search ul li{background: none;}
#result-search ul li a{
  text-transform: none;
  font-weight: normal;
}
#result-search ul li a:hover, #result-search ul li.active a{background: #F0E4DC;}
#result-search ul li:first-child{
  background: url(../images/search-b-name.png) repeat-x 0 0;
  color: #8b8a8a;
  font: 11px/28px $font-f;
  font-weight: $bold-text;
  text-transform: uppercase;
}
#result-search ul li span{
  font: 11px/18px $font-f;
  color: #616161;
}
#result-search ul .bold-text a{font-weight: $bold-text !important;}
/*search end*/


/*casino-search*/
.search-results-inner.casino #casino-result-search ul:hover, .search-results-inner.casino #casino-result-search ul.active{ background: #F0E4DC;}
