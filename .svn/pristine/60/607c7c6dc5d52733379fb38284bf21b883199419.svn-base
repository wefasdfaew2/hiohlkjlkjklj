.history-list ul li.rezult-rate {
  color: #282828;
  font: 14px/16px $font-f;
}
.history-list ul li.rezult-rate span {
  text-transform: uppercase;
  font-weight: $bold-text;
}
.history-list ul li.rezult-rate p {font-weight: $bold-text;}
.history-list ul li.rezult-rate .won-rezult {color: #329a0d;}
.history-list ul li.rezult-rate .lost-rezult {color: $red-text;}
.history-list ul li.rezult-rate .unsettled-rezult {color: #b7b7b7;}
