        box-shadow: none;
        color: #737888;//$text-default-color;
        border-color: transparent;
        &:last-child{border: none;}
      }
    }
    li{
      box-shadow: none;
      background: #232630;//$market-button;
      color: #fff;
      &:first-child{
        border: 0 none;
        border-radius: 3px 0 0 3px;
      }
      &:last-child{
        border-right: 1px solid #2e323e;//$buttons-classic;
        border-radius: 0 3px 3px 0;
      }
      &.disabled-cell{box-shadow: none;}
      &.no-odd,&.no-odd-both{
        box-shadow: none; border-radius: 0 3px 3px 0; background: #232630;// $market-button;
      }
      &.odds{
        .bets{
          border: 1px solid #2e323e;//$buttons-classic;
          background: #232630;//$market-button;
          border-radius: 3px;
          &:hover, &.active{
            background:$red-bg;//$buttons-active-classic;
            border: 1px solid #ba4785;//$buttons-active-stroke;
          }
        }
      }
    }
  }
}


/*virtual sports*/
.gameing-wrapper.prematch .centr-market-contain.virtual-games .scroll-contain{background: #171c2c;}
