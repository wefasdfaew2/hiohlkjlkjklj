.fb-timer-renew ul li:first-child{margin: 0;}
.fb-timer-renew p{
	width: 64px;
	height: 64px;
	background: #656566;
	border-top: 1px solid #767677;
	border-bottom: 3px solid #4d4c4f;
	box-shadow: 0 0 5px $black-bg;
}
.fb-timer-renew ul li:before, .fb-timer-renew ul li:after{
	content: "";
	display: block;
	width: 2px;
	position: absolute;
	top: 28px;
	height: 8px;
	background: #b6b6b6;
	border: 1px solid #454545;
	z-index: 55;
}
.fb-timer-renew ul li p:before{
	content: "";
	display: block;
	width: 100%;
	position: absolute;
	top: 1px;
	height: 31px;
	background: #5a585a;
	z-index: 11;
	left: 0;
}
.fb-timer-renew ul li:before{
	left: 0;
	border-left: none;
}
.fb-timer-renew ul li:after{
	right: 0;
	border-right: none;
}
.fb-timer-renew ul li p span{
	position: absolute;
	width: 100%;
	height: 100%;
	font: 42px/64px $font-f;
	color: $white-bg;
	text-shadow: 0 1px 0 #111111;
	text-align: center;
	z-index: 66;
}
.fb-timer-renew ul li p:after{
	content: "";
	display: block;
	border-top: 1px solid #39383c;
	border-bottom: 1px solid #7a797b;
	width: 58px;
	position: absolute;
	top: 32px;
	left: 3px;
	z-index: 33;
}
.fb-timer-renew ul li p span:before{
	content: ":";
	position: absolute;
	top: 7px;
	left: -18px;
	z-index: 33;
	font: 42px/44px $font-f;
	color: $white-bg;
	text-shadow: 0 1px 0 #111111;
}
.fb-timer-renew ul li:first-child p span:before{display: none;}
