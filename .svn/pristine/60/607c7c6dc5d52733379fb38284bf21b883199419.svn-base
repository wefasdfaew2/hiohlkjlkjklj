  font: 16px/20px $font-f;
  color: #9E9E9E;
  text-shadow: 1px 1px 1px $shadow-color;
}
.casier-volute-name h4 span {
  color: $white-text;
}
.form-sign-in.cashier table {  }
.form-sign-in.cashier table tr th {
  background: #1E2639;
  font: 14px/16px $font-f;
  color: $white-text;
  font-weight: $bold-text;
  text-transform: uppercase;
  border: 1px solid #1b2133;
  border-bottom: 2px solid #1b2133;
}
.form-sign-in.cashier table tr td {
  font: 14px/16px $font-f;
  color: $white-text;
  background: #171c2c;
  box-shadow: 0 0 1px #1E2639;
}
.form-sign-in.cashier table tr td:first-child {
  text-transform: uppercase;
  background: #121623;
}
.form-sign-in.cashier table tr td a {
  background: $red-bg;
  font: 14px/16px $font-f;
  color: $white-text;
  border-radius: 15px;
}
.contain-cashier-forms .select-block {}
.cashier-button button {
  background: $bg-button;
  font: 14px $font-f;
  color: $white-text;
  padding: 0 16px;
  border-radius: 20px;
  height: 36px;
}
.cashier-button button:disabled {background: $bg-button-disable;}
/*cashier END*/

/*recentbets*/
.slider-recentbets .arrow-left-s, .slider-recentbets .arrow-right-s {background: none;}
.slider-recentbets ul li {
  background: $white-bg;
  box-shadow: 0 0 5px $shadow-color;
}
.type-game {
  color: #989898;
  font: 13px/21px $font-f;
  font-weight: $bold-text;
}
.type-game p {
  text-transform: uppercase;
  font: 12px/16px $font-f;
  font-weight: $bold-text;
  color: #329a0d;
}
/*.mini-icon-1 {
  background: url(../images/small-sport-icons-sprite.png) no-repeat -2px -100%;
}*/
.mini-icon-1.mini-icon-2 {background: url(../images/betslip-sprite.png) no-repeat 50% -38px;}
.mini-icon-1.mini-icon-3 {background: url(../images/betslip-sprite.png) no-repeat 50% -157px;}
.mini-icon-1.mini-icon-4 {background: url(../images/betslip-sprite.png) no-repeat 50% 4px;}
.mini-icon-1.mini-icon-848, .game-team-tennis .chempionship-name {background-position: -2px -1085px;}
.type-rezult, .type-rezult-won, .type-rezult-lost {
  font: 12px /21px $font-f;
  font-weight: $bold-text;
  color: $white-text;
  text-transform: uppercase;
  box-shadow: 0 0 2px $shadow-color;
  text-overflow: ellipsis;
  overflow: hidden;
}
.type-rezult {
  background: #FFE000;
  color: $black-text;
}
.type-rezult-won {background: #329a0d;}
.type-rezult-lost {background: #b7b7b7;}
.amount-maney {background: #f2f2f2;}
.amount-maney p {
  color: #888888;
  font: 12px/26px $font-f;
}
.amount-maney span {
  color: $red-text;
  font: 11px/26px $font-f;
  font-weight: $bold-text;
}
.factor-number {
  font: 22px/28px  $font-f;
  color: $black-text;
  white-space: nowrap;
  overflow: hidden;
}
.type1 .factor-number span:after, .type2 .factor-number span:after {content: " /";}
.deposit-alert-contain.casino .games-slider ul li .popular-game-name p a{color: $gray;}
.deposit-alert-contain.casino .games-slider .popular-game-name .fav-block a:before{color: $gray;}
.deposit-alert-contain.casino .games-slider .popular-game-name .fav-block a.star-active:before{color: #efbf00;}
.history-team-name {
  color: $black-text;
  font: 13px/16px $font-f;
  font-weight: $bold-text;
  display: table;
}
.tournament-name {
  color: $black-text;
  font: 10px/16px $font-f;
  font-weight: $bold-text;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.tournament-name span {
  font-weight: normal;
  color: #616161;
}
.tournament-name p {
  float: right;
  font: 11px/16px $font-f;
  color: #999999;
  font-weight: normal;
}
.tournament-name p::selection {
  color: $white-text;
  background: #63454F;
}
.tournament-name p::-moz-selection {
  color: $white-text;
  background: #63454F;
}
.more-events {
  font: 11px/28px $font-f;
  color: $black-text;
  text-transform: uppercase;
}
.more-events span {
  font: 11px/28px $font-f;
  font-weight: $bold-text;
}
.block-more-event {background: #f2f2f2;}
.block-more-event ul li {
  box-shadow: none;
  background: #dddddd;
}
.block-more-event ul li:hover, .block-more-event ul li.open {background: #F0B0B0;}
.block-more-event ul li p {
  font: 12px/24px $font-f;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: $shadow-color;
}
.more-events .market-arrow, .block-more-event ul li .market-arrow {background: url(../images/match-arrow.png) no-repeat 50% 0;}
.result-bet-icon.win, .popup-bet-icon.win {background: url(../images/rezult-icon.png) no-repeat 0 0;}
.result-bet-icon.lost, .popup-bet-icon.lost {background: url(../images/rezult-icon.png) no-repeat 0 -25px;}
.result-bet-icon.watch, .popup-bet-icon.watch {background: url(../images/rezult-icon.png) no-repeat 0 -49px;}
.block-more-event ul li ul li {background: #DBCFCF;}
.block-more-event ul li ul li .my-bets-pick {font-weight: $bold-text;}
.block-more-event ul li ul li:hover {
  background: #DBCFCF;
  cursor: text;
}
.block-more-event ul li ul li p {color: #753E42;}
/*recentbets END*/

/*bet-history*/
.left-block-history h3 {
  color: $white-text;
  font: 22px/30px $font-f;
  font-weight: $bold-text;
  text-shadow: 0 2px 0 #444444;
}
.left-block-history .form-sign-in {
  background: none;
  box-shadow: none;
}
.All-Bets-history {background: $white-bg; }
.All-Bets-history h3 {
  color: $black-text;
  font: 28px/30px $font-f;
  font-weight: $bold-text;
}
.pagination ul li {
  font: 16px/41px $font-f;
  color: $white-text;
  font-weight: $bold-text;
  text-transform: uppercase;
}
