@import 'functions/colors';
@import 'functions/header-and-layout-additional-variables';
@import '../../../css/classic';
body {
  font: 12px $font-f;
  background: $white-bg;
}
/*header*/
.conteiner {box-shadow: 0 0 3px #333333;}
.header-bg{background: $red-bg;}
.conteiner.show-fix .header-bg,
.conteiner.show-game .header-bg{display: block; box-shadow: none;}
@media screen and (max-width: 1850px){
  .category-nav ul li:nth-child(n+9) a{background: $red-bg;}
  .category-nav ul li.active:nth-child(n+9) a{background: $header-nav-bg;}
}
.conteiner.show-fix .logo, .conteiner.show-game .logo {
  width: 315px;
  height: 41px;
}
.conteiner.show-fix .logo a, .conteiner.show-game .logo a {
  background: url(../images/logo2.png) no-repeat 0 50%;
  width: 315px;
  height: 41px;
  padding: 0;
}
.conteiner.show-fix .logo.VBET,  .conteiner.show-game .logo.VBET{ width: 70px;}
.conteiner.show-fix .logo.VBET a,  .conteiner.show-game .logo.VBET a{
  background: url(../images/logo-vbet2.png) no-repeat 0 50%;
  width: 70px;
}
.top-black {background: $black-bg;}
.conteiner.show-fix .block-search, .conteiner.show-game .block-search {}
/*header end*/
.clock {margin: 0 0 0 400px;}
.VBET .clock {margin: 0 0 0 120px;}
.clock span {
  font: 13px/14px $font-f;
  color: $header-text;
  text-transform: uppercase;
  white-space: nowrap;
  font-weight: bold;
}
.conteiner.show-fix .clock, .conteiner.show-game .clock {display: none;}
.topmenu-sport-and-region {
  color: $white-text;
  font: 21px/24px $font-f;
  left: 336px;
  top: 6px;
}
.VBET .topmenu-sport-and-region{ left: 110px;}
.language-change p {
  color: $white-text;
  text-transform: uppercase;
  font: 14px/34px $font-f;
  font-weight: bold;
}
.arrow-lang {
  top: 10px;
  right: 3px;
  transform: rotate(0deg);
  -moz-transform: rotate(0deg);
  -ms-transform: rotate(0deg);
  -o-transform: rotate(0deg);
  -webkit-transform: rotate(0deg);
}
.arrow-lang:before {
  position: absolute;
  content: "\e66c";
  font: 14px/18px icomoon;
  color: #747780;
}
.open-lang {
  background: $white-bg;
  box-shadow: 0 0 15px $shadow-color;
}
.set-icon {
  background: none;
  width: 48px;
  height: 48px;
  position: relative;
  top: -6px;
  right: 6px;
}
.settings-button.open .open-lang{right: 6px;}
.set-icon:before {
  content: "\e6a6";
  font: 48px/48px icomoon;
  color: #fff;
  position: absolute;
  left: 0;
  top: 0;
  width: 48px;
  height: 48px;
}
.list-bet-block .set-icon{top: 0;}
.right-betslip-contain .list-bet-block .set-icon{top: -3px;}
.list-bet-block .set-icon:before{display: none;}
.open-lang .arrow-sport {background-size: contain;}
.settings-button .open-lang p {
  font: 13px/15px $font-f;
  font-weight: $bold-text;
  background: #dddddd;
}
.settings-button:before{
  content: "";
  display: block;
  position: absolute;
  left: -2px;
  height: 14px;
  width: 1px;
  background: #393d4b;
  top: 10px;
}
.help-b {
  position: relative;
}
.help-b:after{
  content: "";
  display: block;
  position: absolute;
  right: 4px;
  height: 14px;
  width: 1px;
  background: #393d4b;
  top: 23px;
}
.conteiner.show-fix .help-b:after, .conteiner.show-game .help-b:after{display: none;}
.help-b.faq:after{right: -5px;}
.help-b.faq{ background: none;}
.help-b.faq:before{
  content: "\e6a4";
  font: 52px/20px icomoon;
  color: $white-bg;
  text-indent: 0;
  position: absolute;
  left: 0;
  top: 20px;
}
.open-lang ul li {font: 13px/15px $font-f;}
.open-lang ul li:hover {color: $red-text;}
.open-lang ul li.active-format {
  color: $red-text;
  background: #eeeeee url(../images/active-format.png) no-repeat 6px 50%;
}
.help-b a { background: none;position: relative; text-decoration: none;}
.help-b a:before{
  content: "\e6c9";
  font: 46px/30px icomoon;
  color: #ffffff;
  text-indent: 0;
  left: 6px;
  top: 16px;
  position: absolute;
}
.form-contain .help-b a:before{
  left: auto;
  right: 2px;
  top: 12px;
}
.prof-name{margin-top: 12px;}
.prof-name a {
  color: $white-text;
  font: 13px/16px $font-f;
  font-weight: bold;
  background: #3a3e4b;
  padding: 10px 30px 10px 10px;
  float: right;
  border-radius: 5px;
  min-height: 16px;
}
.prof-name a:before{
  position: absolute;
  content: "\e66c";
  font: 14px/18px icomoon;
  color: $white-text;
  right: 9px;
}
.prof-name ul {
  background: $white-bg;
  box-shadow: 0 0 15px $shadow-color;
  top: 36px;
  &:before {
    border-color: transparent transparent $white-bg transparent;
  }
}
.main-nav ul li.my-bets a {
  padding-right: 0px;
}
.main-nav ul li.fav-games a, .main-nav ul li a, .main-nav ul li span{ border: none;}
.main-nav ul li a:before,
.main-nav ul li span:before,
.main-nav ul li.balance:before{
  content: "";
  display: block;
  position: absolute;
  left: 0;
  height: 14px;
  width: 1px;
  background: $red-bg;
  top: 15px;
}
.main-nav ul li.balance:after {
  content: "\e6a5";
  font: 48px/40px icomoon;
  color: #a61f67;
  position: absolute;
  left: -5px;
  top: 0;
}
.main-nav ul li.balance a:before{ display: none;}
.main-nav ul li:first-child a:before,
.main-nav ul li:first-child span:before,
.main-nav ul li span.my-games-count:before{ display: none;}
.main-nav ul li.my-bets a p {
  max-width: 0px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.sign-in-reg-buttons{
  position: absolute;
  top: -48px;
  right: 215px;
  width: 280px;
}
.main-nav ul li.register-button,
.main-nav ul li.sign-in-button{
  float: right;
  border-radius: 5px;
  margin: 0 2px;
}
.main-nav ul.not-sign .sign-in-reg-buttons li.register-button span,
.main-nav ul.not-sign .sign-in-reg-buttons li.sign-in-button span{
  padding: 8px 10px !important;
  color: $white-text;
  font: 14px/16px $font-f;
  font-weight: $bold-text;
}
.main-nav ul.not-sign .sign-in-reg-buttons li span:before{
  display: none;
}
.main-nav ul .sign-in-reg-buttons li .active-s{ background: none !important;}
.main-nav ul li.register-button{
  background: #1d7d34;
  border: 2px solid #1d7d34;
}
.main-nav ul li.sign-in-button {
  background: #171c2c;
  border: 2px solid $red-bg;
}
.prof-name ul li {
  border-top: 1px solid #eee;
  color: $lite-black-text;
  font: 13px/16px $font-f;
  background: none;
  &:first-child {
    background: #eee;
    font-weight: bold;
    cursor: default;
    &:hover {
      color: $lite-black-text;
    }
  }
  &.message-icon {
    background: url("../images/user-icons.png") no-repeat 8px 11px;
  }
  &.settings-icon {
    background: url("../images/user-icons.png") no-repeat 8px -26px;
  }
  &.logoff-icon {
    background: url("../images/user-icons.png") no-repeat 8px -62px;
  }
}
.prof-name ul li:hover {color: $red-text;}
/*logo*/
.logo {
  width: 426px;
  height: 100px;
  margin: 0 0 0 10px;
}
.logo a {
  background: url(../images/logo.png) no-repeat 0 100%;
  width: 426px;
  height: 100px;
}
/*menu*/
.main-nav ul li {
  background: $white-bg;
}
.main-nav ul li.my-games {
  background: $white-bg;
  overflow: hidden;
}
.main-nav ul li.fav-games {
  background: $white-bg;
  overflow: hidden;
}
.main-nav ul li.my-games:before, .main-nav ul li.fav-games:before {
  content: "\e6ab";
  font: 85px/40px icomoon;
  color: #a61f67;
  position: absolute;
  left: -23px;
  top: 0;
}
.animate-star-odd .star-anim, .animate-star-even .star-anim{background: url(../images/nav-icons.png) no-repeat 6px 8px;}
.main-nav ul li .active-s{background: url(../images/arrow-nav.png) no-repeat 50% 100% !important;}
.main-nav ul li.active-s {background: #ffffff url(../images/arrow-nav.png) no-repeat 50% 100% !important;}
.main-nav ul li span {
  color: $black-text;
  text-transform: uppercase;
  font: 12px/14px $font-f;
  background: url(../images/nav-icons.png) no-repeat 10px -42px;
  font-weight: $bold-text;
}
.main-nav ul li a {
  color: $black-text;
  text-transform: uppercase;
  font: 12px/14px $font-f;
  font-weight: $bold-text;
}
.main-nav ul li span.my-games-count {
  background: none;
  font: 9px/9px $font-f;
  font-weight: normal;
  color: $white-text;
}
.main-nav ul li.my-bets {
  background: #ffffff url(../images/brend-icon.png) no-repeat 5px 7px;
  background-size: 16px;
}
.main-nav ul li.balance {background: $white-bg;}
.not-sign li span {background: none !important;}
.conteiner.show-fix .main-nav ul.not-sign li:first-child, .conteiner.show-game .main-nav ul.not-sign li:first-child {background: $white-bg;}
.main-nav ul .last-child a {background: url(../images/nav-icons.png) no-repeat 10px -99px;}
.bottom-arrow {background: url(../images/arrow-w-block.png) no-repeat 0 0;}
.left-b {background: url(../images/bg-left-righ-win.png) no-repeat 0 0;}
.right-b {background: url(../images/bg-left-righ-win.png) no-repeat 0 -72px;}
.centr-text-b {
  background: url(../images/separator-bg-block-win.png) repeat-x 0 0;
  text-transform: uppercase;
  color: $white-text;
  font: 11px/18px $font-f;
  font-weight: $bold-text;
}
.conteiner.show-fix .category-nav, .conteiner.show-game .category-nav  {}
.category-nav ul li {text-transform: uppercase;}
.category-nav ul li a {
  color: $white-text;
  font: 12px/14px $font-f;
  font-weight: bold;
  text-shadow: none;
  &.menu-live {
    font-weight: bold;
  }
}
.category-nav ul li a:hover, .category-nav ul .active a{background: $header-nav-bg;}
@media screen and (max-width: 1850px){
  .conteiner.show-fix .category-nav ul li:nth-child(n+10) a, .conteiner.show-game .category-nav ul li:nth-child(n+10) a{}
  .conteiner.show-fix .category-nav ul li:nth-child(n+10) a:hover, .conteiner.show-game .category-nav ul li:nth-child(n+10) a:hover, .category-nav ul:hover li:nth-child(10):before
  {background: $header-nav-bg;}
}
.conteiner.show-fix .help-b a, .conteiner.show-game .help-b a  {
  background: none;
  background-size: 15px !important;
  width: 26px;
}
.conteiner.show-fix .help-b a:before, .conteiner.show-game .help-b a:before{top: 7px; left: -6px;}
.conteiner.show-fix .help-b.faq, .conteiner.show-game .help-b.faq {
  background-size: 30px;
  width: 40px;
}
.conteiner.show-fix .help-b.faq:before, .conteiner.show-game .help-b.faq:before{
  font: 40px/20px icomoon;
  left: 1px;
  top: 12px;
}
/*slider*/
.block-slider {
  background: #121623;
  box-shadow: 0 1px 10px #0D0F18;
}
.closed-arrow-b {
  background: #121623;
  box-shadow: 0 1px 0 #171C2C inset;
}
.closed-arrow-b a {color: $white-text;}
.closed-arrow-b a:before{
  content: "\e671";
  font: 48px/42px icomoon;
  color: #808389;
  position: absolute;
  display: block;
  width: 32px;
  top: 0;
  left: calc((100% - 48px) / 2);
  text-indent: 0;
}
/*slider end*/

/*menu-games*/
.menu-sport {background: url(../images/bg-menu-sport.png) repeat-x 0 0;}
.menu-sport .menu-sport-contain ul .active {background: url(../images/bg-menu-sport.png) repeat-x 0 -41px;}
.menu-sport .menu-sport-contain ul li a {
  color: #8b8a8a;
  font: 11px/14px $font-f;
  text-transform: uppercase;
  font-weight: $bold-text;
}
.menu-sport .menu-sport-contain ul .active a {
  color: $white-text;
  background: url(../images/sport-nav-hover.png) no-repeat 50% 0;
}
.menu-sport .menu-sport-contain ul li a:hover {color: $white-text;}
/*menu-games*/
.menu-games {background: url(../images/bg-menu-games.png) repeat-x 0 -6px;}
.menu-games .contain-menu-games ul li {background: url(../images/bg-sport-nav-a.png) no-repeat 100% 0;}
.menu-games .contain-menu-games ul li.active {
  background: #A61F67;
  box-shadow: 0 -2px 0 #9A113C inset;
}
.menu-games .contain-menu-games ul li a {
  color: $black-text;
  text-transform: uppercase;
  font: 11px/14px $font-f;
  font-weight: $bold-text;
  text-shadow: 1px 1px 1px $white-text;
}
.more-count.ng-binding:before {content: "+";}
.menu-games .contain-menu-games ul li.active a {
  color: $white-text;
  background: url(../images/arrow-sport-m.png) no-repeat 50% 100%;
  text-shadow: 1px 1px 1px $shadow-color;
}
.menu-games .contain-menu-games ul li:first-child:hover {
  /*background: url(../images/bg-sport-nav-a.png) no-repeat 0 0;*/
}
.menu-games .contain-menu-games ul li:first-child a {background: none;}
.menu-games .contain-menu-games ul li.active:first-child {}
.menu-games .contain-menu-games ul li.active:first-child a {background: url(../images/arrow-sport-m.png) no-repeat 50% 100%;}
/*sport-icons*/
.menu-games .contain-menu-games ul li.sport-favorites a span {background: url(../images/fav-star.png) no-repeat 50% 50%;}
.menu-games .contain-menu-games ul li.sport-favorites.active a span {background: url(../images/fav-star-w.png) no-repeat 50% 50%;}
.menu-games .contain-menu-games ul li a span{
  background-image: url(../images/black-icons.png);
  background-repeat: no-repeat;
}
.menu-games .contain-menu-games ul li a span .game-count {background: none;}
